<ion-header>
    <nav class="navbar navbar-default">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">
                로고
            </a>
        </div>
        <input type="checkbox" id="navigation" class="hamburger-toggle" />
        <label class="hamburger-label" for="navigation">
            <span class="hamburger-icon">
                <img src="/assets/images/menu_icon.png" alt="">
            </span>
        </label>
        <div class="side-nav">
            <div class="tool-bar">
                Menu
            </div>
            <div class="accordion">
                <div class="accordion-item">
                    <h2 class="accordion-header">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#menu1">
                            운세
                        </button>
                    </h2>
                    <div id="menu1" class="accordion-collapse collapse">
                        <div class="accordion-body">

                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#menu2">
                            사주
                        </button>
                    </h2>
                    <div id="menu2" class="accordion-collapse collapse">
                        <div class="accordion-body">

                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h2 class="accordion-header">
                        <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#menu3">
                            타로
                        </button>
                    </h2>
                    <div id="menu3" class="accordion-collapse collapse">
                        <div class="accordion-body">
                            3
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <div class="menu">
        <ion-slides [options]="slideOpts">
            <ion-slide>추천</ion-slide>
            <ion-slide class="active">운세</ion-slide>
            <ion-slide>사주</ion-slide>
            <ion-slide>타로</ion-slide>
            <ion-slide>궁합</ion-slide>
            <ion-slide>18+</ion-slide>
        </ion-slides>
    </div>
